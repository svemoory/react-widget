{
  "name": "react-biwidget",
  "version": "1.0.15",
  "description": "A React component to generate widget",
  "keywords": [
    "react",
    "react-component"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "lint": "eslint src",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch",
    "build": "cross-env NODE_ENV=production sh ./build-tools/build.sh",
    "compile": "cross-env NODE_ENV=production webpack --config build-tools/webpack.config.prod.js --progress",
    "buildSite": "cross-env NODE_ENV=production webpack --config build-tools/webpack.config.site.js --progress",
    "prepublish": "npm run build && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svemoory/react-widget"
  },
  "author": "SNV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/svemoory/react-widget/issues"
  },
  "homepage": "https://github.com/svemoory/react-widget#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "cross-env": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "c3": "^0.4.11",
    "css-loader": "^0.28.2",
    "file-loader": "^0.11.1",
    "lodash": "^4.17.4",
    "numeral": "^2.0.6",
    "react-mlsdata": "^0.5.5",
